<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Yuriy Lyeshchenko</Authors>
    <Description>Extension methods to simplify writing unit tests for code written using LanguageExt library</Description>
    <PackageProjectUrl>https://github.com/yuriylsh/LanguageExt.UnitTesting</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/yuriylsh/LanguageExt.UnitTesting/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/yuriylsh/LanguageExt.UnitTesting</RepositoryUrl>
    <RepositoryUrl>https://github.com/yuriylsh/LanguageExt.UnitTesting.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>LanguageExt</PackageTags>
    <PackageReleaseNotes>Adding non-validating extenstion method overloads (thanks @ovstetun for the contribution).</PackageReleaseNotes>
    <Copyright>Yuriy Lyeshchenko © 2018</Copyright>
    <AssemblyVersion>3.3.5.0</AssemblyVersion>
    <FileVersion>3.3.5.0</FileVersion>
    <Version>3.3.5</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LanguageExt.Core" Version="3.3.5" />
  </ItemGroup>

</Project>
